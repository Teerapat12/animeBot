<script>
//LoadFilm(1,1);
LoadFilmOngoing(1);
$(document).on("click", ".pagination.recent a", function(e){
        e.preventDefault();
		var type = $('#type').val();
		var str = (this).href;
		str = str.split("=");
		var page = str[1];
        LoadFilm(page,type);
});
$(document).on("click", ".pagination.popular a", function(e){
        e.preventDefault();
		var str = (this).href;
		str = str.split("=");
		var page = str[1];
        LoadFilmOngoing(page);
});
$(document).on("click", "a.dub", function(e){
        e.preventDefault();
		var type = this.rel;
         if ( !$( this ).hasClass( "active" ) ) {
            LoadFilm(1,type);
         }
});
function LoadFilm(page,id){
    $('#load_recent_release').stop(true,true).load(base_url + '/page-recent-release.html?page=' + page + '&type=' + id);
}
function LoadFilmOngoing(page){
    $('#load_popular_ongoing').stop(true,true).load(base_url + '/page-recent-release-ongoing.html?page=' + page);
}
</script>

